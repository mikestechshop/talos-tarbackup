<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.jwt.tarbackup.fap6</string>
	<key>OnDemand</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/Scripts/JWT/tarbackup/tarbackup.sh</string>
		<string>-v</string>
		<string>4</string>
		<string>-l</string>
		<string>/Library/Logs/JWT/tarbackup-fap6.log</string>
		<string>/Library/FAPServer</string>
		<string>/Volumes/Share1/Backups/FAPServer</string>
		<string>36</string>
	</array>
	<key>RunAtLoad</key>
	<false/>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>15</integer>
		<key>Minute</key>
		<integer>0</integer>
		<key>Weekday</key>
		<integer>6</integer>
	</dict>
	<key>UserName</key>
	<string>root</string>
</dict>
</plist>
